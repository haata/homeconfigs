keycode 0x4F =	Home		KP_7		Home		KP_7
keycode 0x50 =	Up		KP_8		Up		KP_8
keycode 0x51 =	Prior		KP_9		Prior		KP_9
keycode 0x53 =	Left		KP_4		Left		KP_4
keycode 0x54 =	Down		KP_5		Down		KP_5
keycode 0x55 =	Right		KP_6		Right		KP_6
keycode 0x57 =	End		KP_1		End		KP_1
keycode 0x58 =	Down		KP_2		Down		KP_2
keycode 0x59 =	Next		KP_3		Next		KP_3
keycode 0x4C =	Super_L		NoSymbol	Super_L
keycode 0x42 =	Alt_R		NoSymbol	Alt_R
keycode 0x5A =	Insert		KP_0		Insert		KP_0
keycode 0x5B =	Delete		KP_Decimal	Delete		KP_Decimal

clear Lock
clear Mod4
clear Mod5

add    Mod4    = Super_L Super_L Super_R 0x00CE 0x00CF
add    Mod5    = Alt_R ISO_Level3_Shift Mode_switch
